#!/bin/sh
# Make the output files for each test

# Build our compiler if needed
if [ ! -f ../cwj ]
then (cd ..; make install)
fi

for i in input*c
do if [ ! -f "out.$i" -a ! -f "err.$i" ]
   then
     ../cwj -o out $i 2> "err.$i"
     # If the err file is empty
     if [ ! -s "err.$i" ]
     then
        rm -f "err.$i"
        cc -o out $i
        ./out > "out.$i"
      fi
   fi
   rm -f out out.s
done
